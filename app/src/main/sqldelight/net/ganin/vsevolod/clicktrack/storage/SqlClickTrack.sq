CREATE TABLE ClickTrack (
    name TEXT NOT NULL PRIMARY KEY,
    serialized TEXT NOT NULL
);

getAll:
SELECT * FROM ClickTrack;

getAllNames:
SELECT name FROM ClickTrack;

getByName:
SELECT * FROM ClickTrack WHERE name = (:name);

put:
INSERT OR REPLACE INTO ClickTrack VALUES ?;

remove:
DELETE FROM ClickTrack WHERE name = ?;
